{
  "extends": "./tsconfig.paths.json",
  "compilerOptions": {
    "target": "ES5",
    "module": "ESNext",
    "strictNullChecks": true,
    "esModuleInterop": true,
    "skipDefaultLibCheck": true,
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "downlevelIteration": true,
    "sourceMap": true
  },
  "include": ["src"]
}